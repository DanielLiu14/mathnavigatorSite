@import "../static.styl"

#view-home
  width: 100%
  height: 400px
  background-color: colorWhite

  & div
    display: inline-block
    float:left

#view-home #view-content
    display: inline-block

    & div
      display: inline-block

.boxed
    border: 2px solid black
    width: 360px
    height: 240px
    display: inline-block
    float: right
    position: relative
    left: 100px
    top: 30px
    bottom: 30px

#view-home #go-to-page
      display: inline-block
      position: absolute
      right: 0
      bottom: 0
      margin: spacing2x 0
      background-color: colorTurquoise
      color: white
      padding: 0.75em;
      border-radius: 10px;
